<%- before_html %>
<!DOCTYPE html >
<!--[if IE]><![endif]-->
<!--[if lt IE 7 ]> <html lang="en" class="ie6">    <![endif]-->
<!--[if IE 7 ]>    <html lang="en" class="ie7">    <![endif]-->
<!--[if IE 8 ]>    <html lang="en" class="ie8">    <![endif]-->
<!--[if IE 9 ]>    <html lang="en" class="ie9">    <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!-->
<%- html_start_tag %><!--<![endif]-->
    <head <%- head_attribute %>>
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <meta name="description" content="" />
        <meta name="author" content="" />  
        <link rel="apple-touch-icon" sizes="180x180" href="<%- asset_path("apple-touch-icon.png") %>">
        <link rel="icon" type="image/png" href="<%- asset_path("favicon-32x32.png") %>" sizes="32x32">
        <link rel="icon" type="image/png" href="<%- asset_path("favicon-16x16.png") %>" sizes="16x16">
        <link rel="shortcut icon" href="<%- asset_path("favicon.ico") %>">
        <link rel="manifest" href="<%- asset_path("manifest.json") %>">
        <link rel="mask-icon" href="<%- asset_path("safari-pinned-tab.svg") %>" color="#5bbad5">
        <meta name="theme-color" content="#ffffff">
        <!-- Bootstrap CSS -->
        <%- cdn_top %>
        <meta name="GENERATOR" content="Microsoft SharePoint" />
        <meta http-equiv="Content-type" content="text/html; charset=utf-8" />
        <meta http-equiv="Expires" content="0" />
        <%- head_end %>
        <!-- Template CSS -->
        <%- asset_path("lcc-template.css") %>
        <!-- Application CSS -->
        <%- application_css %>
        </head>
    <body <%- body_attribute %>>
    <%- body_start %>
        <div id="skiplink-container">
            <div>
                <a href="#main" class="skip">Skip to main content</a>
            </div>
        </div>
        <div id="s4-workspace-disabled" class="mainWorkspace">
            <div id="s4-bodyContainer">
                <!-- COOKIES -->
				<div id="containerSiteStatement">
                    <div id="siteStatement">
                        <div id="global-cookie-message" class="pageControl container-fluid noindex" style="display:none">
                            <p>Cookies - We use cookies to help improve your experience of using our website. For more information about the cookies we use see our <a href="/Pages/PrivacyStatement.aspx">privacy page</a>. If you continue without changing your cookie settings we assume that you are happy with our use of cookies. You can change your cookie settings at any time by following the instructions on our <a href="/Pages/PrivacyStatement.aspx">privacy page</a>  </p>
                            <a class="submit js-seen-cookie-message" href="">Don't show this message again</a>
                        </div>
                    </div>
            </div> 
                <!--masterhead section -->
                <section class="masterhead">
                    <div class="container-fluid">
                        <div class="">
                            <a class="navbar-brand" href="/">
                            </a>
                            <div class="pull-right">
                                <ul class="menu">
                                    <li>
                                        <div class="nav-search" id="nav-search">
                                            <div class="">
                                                <div class="input-group">
                                                    <!--sharepoint search-->
                                                    <div data-name="search_box">
                                                          <%- search_box %>                                          
                                                    </div>
                                                    <!-- end sharepoint search -->
                                                </div>
                                            </div>
                                        </div>
                                    </li>
                                    <li>
                                        <a class="search" href="#">
                                            <span class="sr-only">Toggle website search
                                            </span>
                                        </a>
                                    </li>
                                    <li>
                                        <a class="main-menu" href="#">
                                            <span class="sr-only">Toggle main menu
                                            </span>Menu</a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </section>
                <!--/.masterHead section -->
                <!-- Header -->
                <header class="header-nav-container"> 
                <div id="header">
                    <section class="sectionHeader">
                        <div class="container-fluid">
                            <div class="col-sm-6 col-xs-12 removeSidePadding header-left">
                             <%- logo %>
                            </div>
                            <div class="col-sm-6 col-xs-12 removeSidePadding header-right">
                            </div>
                        </div>
                    </section>
                    </div>
                    <!-- main nav -->
                    <nav id="main-menu" class="main-nav">
                        <div class="container-fluid">
                            <!-- sharepoint nav -->
                            <div data-name="main_nav">
                            <%- main_nav %>
                            </div>
                            <!--/.sharepoint nav -->
                        </div>
                        <!-- /.container -->
                    </nav>
                    <!-- /.main-menu -->
                </header>
                <!-- /.header-nav-container -->

                <!--[if (lt IE 9) & (!IEMobile)]>
                    <![if (IE 8)]>
                        <div class="browser-alert"><div class="container-fluid"><div class="row"><div class="col-lg-12"><span>This site is optimised for modern web browsers, and does not fully support your version of Internet Explorer</span></div></div></div></div>
                    <![endif]>

                    <![if (IE 7)]>
                        <div class="browser-alert"><div class="container-fluid"><div class="row"><div class="col-lg-12"><span>This site is optimised for modern web browsers, and does not fully support your version of Internet Explorer</span></div></div></div></div>
                    <![endif]>

                    <![if (IE 6)]>
                        <div class="browser-alert"><div class="container-fluid"><div class="row"><div class="col-lg-12"><span>This site is optimised for modern web browsers, and does not fully support your version of Internet Explorer</span></div></div></div></div>
                    <![endif]>
                <![endif]-->

                  <!-- ALERTS -->
                <div id="alert">
                <%- layout_alerts %>
                </div>
                 <!-- /ALERTS -->

                <!-- MAIN CONTENT -->
                <div data-name="bodycontent">
                  <%- body_content %>
                </div>
                <!-- /MAIN CONTENT -->
                <!-- footer -->
                <footer class="main">
                    <section class="footerGlobal">
                        <div class="container-fluid">
                            <!-- build:footerCustom -->
                            <%-  layout_reuserblecontent_footer %>
                            <!-- endbuild -->
                            <div id="GoogleAnalytics"></div>
                        </div>
                        <!-- /.row -->
                    </section>
                </footer>
                <!-- BACK TO TOP -->
                <a href="#0" class="cd-top" data-module="back-to-top">Top</a>         
            </div>
        </div> 
        <!-- webchat modal popup placeholder - legacy code. maybe worth changing in the future but changes to the engagement engine would be needed -->
        <div id="myDiv"></div>
        <%- body_end %>
        <%- cdn_bottom %>
        <script src="<%= asset_path("lcc-template.js") %>" type="text/javascript"></script>
        <script src="<%= asset_path("application.js") %>" type="text/javascript"></script>  
        <%-  layout_additional_page_scripts %>  
   </body>
<%- html_end_tag %>